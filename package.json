{
  "name": "pinggy-test",
  "version": "1.0.0",
  "description": "Test for pinggy native module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "tsc",
    "package": "node-pre-gyp package",
    "publish-binary": "node-pre-gyp publish",
    "test": "jest",
    "start": "node dist/index.js",
    "postinstall": "node ./copy.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pinggy-io/sdk-nodejs.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "node-api-headers": "^1.5.0",
    "node-pre-gyp-github": "^2.0.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.0"
  },
  "binary": {
    "module_name": "addon",
    "module_path": "./lib/",
    "host": "https://github.com/Pinggy-io/sdk-nodejs/releases/download/",
    "remote_path": "{version}"
  }
}
