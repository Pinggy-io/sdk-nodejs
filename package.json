{
  "name": "@pinggy/pinggy",
  "version": "0.1.3",
  "description": "Test for pinggy native module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "build": "tsup",
    "build-native": "node-gyp clean configure build && node ./copy.js",
    "package": "node-pre-gyp package",
    "publish-binary": "node-pre-gyp publish",
    "test": "jest",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pinggy-io/sdk-nodejs.git"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "node-api-headers": "^1.5.0",
    "node-pre-gyp-github": "^2.0.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.0",
    "adm-zip": "^0.5.16",
    "follow-redirects": "^1.15.9",
    "shlex": "^2.1.2",
    "tar": "^7.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "binary": {
    "module_name": "addon",
    "module_path": "./lib/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://github.com/Pinggy-io/sdk-nodejs/releases/download/{version}/"
  }
}
